<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="UIView">
        <attr name="view.materialDesign" format="boolean" />
        <attr name="frame.x" format="float" />
        <attr name="frame.y" format="float" />
        <attr name="frame.width" format="float" />
        <attr name="frame.height" format="float" />
        <attr name="constraint.enabled" format="boolean" />
        <attr name="constraint.alignmentRelate" format="enum">
            <enum name="RelateToGroup" value="0" />
            <enum name="RelateToPrevious" value="1" />
        </attr>
        <attr name="constraint.centerHorizontally" format="boolean" />
        <attr name="constraint.centerVertically" format="boolean" />
        <attr name="constraint.sizeRelate" format="enum">
            <enum name="RelateToGroup" value="0" />
            <enum name="RelateToPrevious" value="1" />
        </attr>
        <attr name="constraint.width" format="string" />
        <attr name="constraint.height" format="string" />
        <attr name="constraint.pinRelate" format="enum">
            <enum name="RelateToGroup" value="0" />
            <enum name="RelateToPrevious" value="1" />
        </attr>
        <attr name="constraint.top" format="string" />
        <attr name="constraint.left" format="string" />
        <attr name="constraint.right" format="string" />
        <attr name="constraint.bottom" format="string" />
        <attr name="view.maxWidth" format="float" />
        <attr name="view.automaticallyAdjustsSpace" format="boolean" />
        <attr name="marginInset.top" format="float" />
        <attr name="marginInset.left" format="float" />
        <attr name="marginInset.right" format="float" />
        <attr name="marginInset.bottom" format="float" />
        <attr name="view.tintColor" format="color" />
        <attr name="view.wantsLayer" format="boolean" />
        <attr name="layer.backgroundColor" format="color" />
        <attr name="layer.cornerRadius" format="float" />
        <attr name="layer.borderWidth" format="float" />
        <attr name="layer.borderColor" format="color" />
        <attr name="layer.shadowX" format="float" />
        <attr name="layer.shadowY" format="float" />
        <attr name="layer.shadowRadius" format="float" />
        <attr name="layer.shadowColor" format="color" />
        <attr name="layer.clipToBounds" format="boolean" />
        <attr name="view.userInteractionEnabled" format="boolean" />
    </declare-styleable>
    <declare-styleable name="UIControl" parent="UIView">
        <attr name="control.enabled" format="boolean" />
        <attr name="control.selected" format="boolean" />
        <attr name="control.contentVerticalAlignment" format="enum">
            <enum name="Center" value="0" />
            <enum name="Top" value="1" />
            <enum name="Bottom" value="2" />
            <enum name="Fill" value="3" />
        </attr>
        <attr name="control.contentHorizontalAlignment" format="enum">
            <enum name="Center" value="0" />
            <enum name="Left" value="1" />
            <enum name="Right" value="2" />
            <enum name="Fill" value="3" />
        </attr>
    </declare-styleable>
    <declare-styleable name="UIButton" parent="UIControl">
        <attr name="button.title" format="string" />
        <attr name="button.image" format="reference" />
        <attr name="button.contentInsets.top" format="float" />
        <attr name="button.contentInsets.left" format="float" />
        <attr name="button.contentInsets.right" format="float" />
        <attr name="button.contentInsets.bottom" format="float" />
        <attr name="button.titleEdgeInsets.top" format="float" />
        <attr name="button.titleEdgeInsets.left" format="float" />
        <attr name="button.titleEdgeInsets.right" format="float" />
        <attr name="button.titleEdgeInsets.bottom" format="float" />
        <attr name="button.imageEdgeInsets.top" format="float" />
        <attr name="button.imageEdgeInsets.left" format="float" />
        <attr name="button.imageEdgeInsets.right" format="float" />
        <attr name="button.imageEdgeInsets.bottom" format="float" />
    </declare-styleable>
    <declare-styleable name="UIImageView" parent="UIView">
        <attr name="imageView.image" format="reference" />
        <attr name="imageView.contentMode" format="enum">
            <enum name="ScaleToFill" value="0" />
            <enum name="ScaleAspectFit" value="1" />
            <enum name="ScaleAspectFill" value="2" />
            <enum name="Redraw" value="3" />
            <enum name="Center" value="4" />
            <enum name="Top" value="5" />
            <enum name="Bottom" value="6" />
            <enum name="Left" value="7" />
            <enum name="Right" value="8" />
            <enum name="TopLeft" value="9" />
            <enum name="TopRight" value="10" />
            <enum name="BottomLeft" value="11" />
            <enum name="BottomRight" value="12" />
        </attr>
    </declare-styleable>
    <declare-styleable name="UILabel" parnet="UIView">
        <attr name="label.text" format="string" />
        <attr name="label.fontSize" format="float" />
        <attr name="label.textColor" format="color" />
        <attr name="label.numberOfLines" format="integer" />
        <attr name="label.linebreakMode" format="enum">
            <enum name="ByWordWrapping" value="0" />
            <enum name="ByTruncatingHead" value="1" />
            <enum name="ByTruncatingMiddle" value="2" />
            <enum name="ByTruncatingTail" value="3" />
        </attr>
        <attr name="label.alignment" format="enum">
            <enum name="Normal" value="0" />
            <enum name="Center" value="1" />
            <enum name="Opposite" value="2" />
        </attr>
    </declare-styleable>

</resources>
